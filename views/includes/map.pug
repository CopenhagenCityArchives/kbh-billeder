block content
  style.
    #map {
      width: 100%;
      height: 100%;
    }

  #map

  script.
    var map;
    var superchargers = [
      {
        location: 'Reload',
        latitude: 55.67928701351651,
        longitude: 12.554863339160379
      }
    ];

    function initMap() {
      map = new google.maps.Map(document.getElementById('map'), {
        center: { lat: 55.67928701351651, lng: 12.554863339160379 },
        zoom: 15
      });

      superchargers.forEach(function(sc) {
        var marker = new google.maps.Marker({
          position: new google.maps.LatLng(sc.latitude, sc.longitude),
          icon: {
            url: 'data:image/png;base64,',
            scaledSize: new google.maps.Size(172, 162)
          },
          map: map,
          title: sc.location,
          animation: google.maps.Animation.DROP
        });
      });
    }

  script(async defer src=`//maps.googleapis.com/maps/api/js?key=AIzaSyDZpS9njLQsoRhlZTIibM3tRhiehUwbCAE&callback=initMap`)