{
  "name": "kbh-billeder",
  "version": "0.1.0",
  "description": "Configuration/setup for the kbhbilleder.dk webapp built on top of Collections Online",
  "main": "server.js",
  "scripts": {
    "gulp": "gulp",
    "index": "node indexing.js",
    "patch-package-json": "npm install dotenv && node ./package-json-patcher.js",
    "prepublish": "gulp build",
    "start:dev": "concurrently \"npm run gulp\" \"npm run start:nodemon\"",
    "start:nodemon": "nodemon --watch . --watch ../collections-online --watch ../collections-online-cumulus server.js",
    "start:inspect": "kill-port --port 9229 && node --inspect=0.0.0.0:9229 server.js",
    "start:nodemon-inspect": "nodemon --delay 80ms --watch . --watch ../collections-online --watch ../collections-online-cumulus --exec 'npm run start:inspect'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CopenhagenCityArchives/kbh-billeder.git"
  },
  "author": "Socialsquare for the Copenhagen City Archives",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CopenhagenCityArchives/kbh-billeder/issues"
  },
  "homepage": "https://github.com/CopenhagenCityArchives/kbh-billeder#readme",
  "dependencies": {
    "bootstrap-sass": "^3.3.7",
    "collections-online": "git+https://github.com/CopenhagenCityArchives/collections-online.git",
    "collections-online-cumulus": "git+https://github.com/CopenhagenCityArchives/collections-online-cumulus.git",
    "dotenv": "^2.0.0",
    "express": "^4.13.4",
    "express-validator": "^5.3.0",
    "lodash": "~4.17.5",
    "q": "^1.4.1"
  },
  "devDependencies": {
    "concurrently": "^3.6.1",
    "gulp": "^4.0.0",
    "http-server": "^0.11.1",
    "kill-port": "^1.3.2",
    "nodemon": "^1.18.5",
    "run-sequence": "^1.1.5"
  }
}
