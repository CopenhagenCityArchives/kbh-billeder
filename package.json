  "dependencies": {
    "bootstrap-sass": "^3.3.7",
    "collections-online": "git+https://github.com/CopenhagenCityArchives/collections-online.git",
    "collections-online-cumulus": "git+https://github.com/collections-online/collections-online-cumulus.git",
    "dotenv": "^2.0.0",
    "express": "^4.13.4",
    "lodash": "^4.12.0",
    "q": "^1.4.1"
