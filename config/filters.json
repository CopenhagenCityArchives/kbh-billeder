{
  "district": {
    "type": "term",
    "field": "district.raw",
    "singular": "bydel",
    "plural": "bydele"
  },
  "street_name": {
    "type": "term",
    "field": "street_name.raw",
    "size": 20,
    "singular": "gadenavn",
    "plural": "gadenavne"
  },
  "creation": {
    "type": "date-range",
    "field": "creation_time.timestamp",
    "singular": "periode",
    "plural": "perioder"
  },
  "original_material": {
    "type": "term",
    "field": "original_material.displaystring",
    "singular": "originalmateriale",
    "plural": "originalmaterialer"
  },
  "license": {
    "type": "term",
    "field": "license.displaystring",
    "singular": "licens",
    "plural": "licenser"
  },
  "institution": {
    "type": "term",
    "field": "catalog_name.raw",
    "singular": "institution",
    "plural": "institutioner"
  },
  "location": {
    "filters" : {
      "Har placering": {
        "bool": {
          "should": [
            {"exists": { "field": "location.verified.latitude" }},
            {"exists": { "field": "location.crowd.latitude" }}
          ]
        }
      },
      "Mangler placering": {
        "bool": {
          "must_not": [
            {"exists": { "field": "location.verified.latitude" }},
            {"exists": { "field": "location.crowd.latitude" }}
          ]
        }
      }
    },
    "plural": "placeringer",
    "singular": "placering",
    "type": "filters"
  },
  "tags": {
    "filters" : {
      "Har emneord": {
        "bool": {
          "should": [
            {"exists": { "field": "tags" }}
          ]
        }
      },
      "Mangler emneord": {
        "bool": {
          "must_not": [
            {"exists": { "field": "tags" }}
          ]
        }
      }
    },
    "plural": "emneord",
    "singular": "emneord",
    "type": "filters"
  },
  "q": {
    "type": "querystring",
    "skipSplit": true
  }
}
